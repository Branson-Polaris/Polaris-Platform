# 15jan20,pfl  Created
#

#
# DESCRIPTION
#
# This file defines the mimalloc library configuration options
#
##########################################################################

# Kernel heap selection
config MIMALLOC_AS_KERNEL_HEAP
   bool "Enable mimalloc as the kernel heap manager"
   depends on MIMALLOC
   default y
   help
      This option selects mimalloc as the kernel heap manager. Disable this option to fall back to the legacy kernel heap manager.

# RTP heap selection
config MIMALLOC_AS_RTP_HEAP
   bool "Enable mimalloc as the RTP heap manager for all RTPs"
   depends on MIMALLOC
   default y
   help
      This option selects mimalloc as the RTP heap manager to be used for all RTP projects built against the VSB project. Disabling this option will result in allowing the selection of using the legacy heap manager vs. the mimalloc heap manager on a per-RTP basis.

# Advance mimalloc feature for kernel
config MIMALLOC_KERNEL_SECURE
   bool "Enable mimalloc secure mode for kernel"
   depends on MIMALLOC_AS_KERNEL_HEAP
   default n
   help
      This option enables kernel MIMALLOC secure mode feature. There are four levels of support and each level is cumulative of the previous one.

menu "Advanced MIMALLOC Feature for Kernel"
depends on MIMALLOC_KERNEL_SECURE

choice MIMALLOC_KERNEL_SECURE_LEVEL
    prompt "Select secure level"
    depends on MIMALLOC_KERNEL_SECURE
    default MIMALLOC_KERNEL_SECURE_LEVEL1

config MIMALLOC_KERNEL_SECURE_LEVEL1
   bool "Enable MI_SECURE=1"
   help
      This option adds guard page around each heap metadata.

config MIMALLOC_KERNEL_SECURE_LEVEL2
   bool "Enable MI_SECURE=2"
   help
      This option adds guard page around each heap page.

config MIMALLOC_KERNEL_SECURE_LEVEL3
   bool "Enable MI_SECURE=3"
   help
      This option enables free list encoding which detects corrupted free list and invalid pointer free.

config MIMALLOC_KERNEL_SECURE_LEVEL4
   bool "Enable MI_SECURE=4"
   help
      This option enables checking of double free.

endchoice
endmenu

# Advance mimalloc feature for RTPs
config MIMALLOC_RTP_SECURE
   bool "Enable mimalloc secure mode for RTPs"
   depends on MIMALLOC_AS_RTP_HEAP
   default n
   help
      This option enables RTP MIMALLOC secure mode feature. There are four levels of support and each level is cumulative of the previous one.

menu "Advanced MIMALLOC Feature for RTPs"
depends on MIMALLOC_RTP_SECURE

choice MIMALLOC_RTP_SECURE_LEVEL
    prompt "Select secure level"
    depends on MIMALLOC_RTP_SECURE
    default MIMALLOC_RTP_SECURE_LEVEL1

config MIMALLOC_RTP_SECURE_LEVEL1
   bool "Enable MI_SECURE=1"
   help
      This option adds guard page around each heap metadata.

config MIMALLOC_RTP_SECURE_LEVEL2
   bool "Enable MI_SECURE=2"
   help
      This option adds guard page around each heap page.

config MIMALLOC_RTP_SECURE_LEVEL3
   bool "Enable MI_SECURE=3"
   help
      This option enables free list encoding which detects corrupted free list and invalid pointer free.

config MIMALLOC_RTP_SECURE_LEVEL4
   bool "Enable MI_SECURE=4"
   help
      This option enables checking of double free.

endchoice
endmenu


